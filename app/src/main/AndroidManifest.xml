            </intent-filter>
        </activity>
        <activity
            android:name=".home.HomePageActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="nosensor">
        </activity>
        <activity
            android:name=".shoppingmall.ShoppingActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="nosensor">
        </activity>
        <activity
            android:name=".agriculture.find.FindActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="nosensor">
        </activity>

    </application>

</manifest>